<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.HBox?>
<fx:root type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <HBox fx:id="wrapBox" VBox.vgrow="ALWAYS" alignment="CENTER">
        <AnchorPane fx:id="anchor">
            <ImageView fx:id="scoreboard" fitWidth="280" pickOnBounds="true" preserveRatio="true">
            </ImageView>
        </AnchorPane>
    </HBox>
</fx:root>
